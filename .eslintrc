{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },

    "plugins": ["@babel"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "requireConfigFile": false,
        "babelOptions": {
            "plugins": [
                "@babel/plugin-proposal-nullish-coalescing-operator",
                "@babel/plugin-proposal-optional-chaining",
                "@babel/plugin-proposal-private-methods",
                "@babel/proposal-class-properties"
            ]
        }
    },
    "rules": {
        "no-console": 0,
        "quotes": [2, "single"],
        "semi": [2, "always"],
        "block-scoped-var": 0,
        "class-methods-use-this": 0,
        "eqeqeq": 0,
        "no-caller": 0,
        "no-empty-function": 0,
        "no-setter-return": 0,
        "no-dupe-else-if": 0,
        "no-eval": 0,
        "no-implied-eval": 0,
        "no-magic-numbers": 0,
        "no-multi-spaces": 0,
        "no-new-wrappers": 0,
        "no-param-reassign": 0,
        "no-sequences": 0,
        "no-label-var": 0,
        "no-shadow": 0,
        "no-undefined": 0,
        "no-use-before-define": 0,
        "block-spacing": 0,
        "max-params": [0, { "max": 5 }],
        "no-bitwise": 0,
        "no-trailing-spaces": 0,
        "prefer-exponentiation-operator": 0,
        "switch-colon-spacing": [0, { "after": true, "before": false }],
        "arrow-body-style": [0, "as-needed"],
        "arrow-spacing": [0, { "before": true, "after": true }],
        "generator-star-spacing": [0, { "before": true, "after": false }],
        "no-useless-constructor": 0,
        "no-var": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-destructuring": 0,
        "camelcase": 0,
        "linebreak-style": 0,
        "indent": 0,
        "max-len": [0, { "code": 120 }],
        "no-unused-expressions": [
            0,
            {
                "allowShortCircuit": true,
                "allowTernary": true,
                "allowTaggedTemplates": true
            }
        ]
    }
}


